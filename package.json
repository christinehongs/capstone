{
  "name": "capstone",
  "version": "1.0.0",
  "description": "By, Christine Hong, Emily Ye, and Sam Davis",
  "engines": {
    "node": "^16.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-all": "yarn install && concurrently \"yarn client-install\" \"yarn server-install\"",
    "client-install": "cd client && yarn install",
    "server-install": "cd server && yarn install",
    "client-start": "cd client && yarn start",
    "server-start": "cd client && yarn start",
    "start": "concurrently \"yarn client-start\" \"yarn server-start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christinehongs/capstone.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/christinehongs/capstone/issues"
  },
  "homepage": "https://github.com/christinehongs/capstone#readme",
  "dependencies": {
    "concurrently": "^6.5.1"
  }
}
