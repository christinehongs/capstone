  "engines": {
    "node": "^16.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-all": "yarn install && concurrently \"yarn client-install\" \"yarn server-install\"",
    "client-install": "cd client && yarn install",
    "client-build": "cd cllient && yarn build",
    "server-install": "cd api && yarn install",
    "client-start": "cd client && yarn start",
    "server-start": "cd api && yarn start",
    "start": "concurrently \"yarn client-start\" \"yarn server-start\""
  },
